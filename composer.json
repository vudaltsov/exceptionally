{
    "name": "udaltsov/exceptionally",
    "description": "Exceptionally helps converting errors to exceptions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Udaltsov",
            "email": "udaltsov.valentin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "symfony/phpunit-bridge": "^4.3",
        "vimeo/psalm": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Udaltsov\\Exceptionally\\": "src"
        },
        "files": [
            "functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Udaltsov\\Exceptionally\\": "tests"
        }
    },
    "archive": {
        "exclude": [
            "/tests"
        ]
    }
}
